<% data.dictionary.each do |entry| %>
  <div>
    <h2 id=<%="#{entry.word.parameterize}"%>-definition>
      <%= entry.word %>
    </h2>
    <small>Synonyms: <%= entry.synonyms %></small>
    <p><%= entry.description %></p>
  </div>
<% end %>
